# S1 Protocol: Environment Verification for CI/CD
# This file is validated at boot time

# Critical Security
JWT_SECRET=test_secret_key_for_ci_environment_only_32
JWT_EXPIRES_IN=7d

# Database
DATABASE_URL=postgresql://apex:apex@localhost:5432/test

# Redis
REDIS_URL=redis://localhost:6379

# MinIO (S3)
S3_ENDPOINT=http://localhost:9000
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=test
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET_NAME=apex-assets

# Application
NODE_ENV=test
PORT=3000

# S2 Tenant Isolation
TENANT_ISOLATION_MODE=strict

# Rate Limiting (S6)
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# S1 Enforcement
ENABLE_S1_ENFORCEMENT=true
