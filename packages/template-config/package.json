{
    "name": "@apex/template-config",
    "version": "1.0.0",
    "description": "Shared Zod schemas and TypeScript types for template.config.json",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "type-check": "tsc --noEmit",
        "test": "bun test"
    },
    "dependencies": {
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
}